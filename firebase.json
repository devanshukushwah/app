{
  "hosting": {
    "public": "build",
    "predeploy":["npm install","npm i firebase","npm run build"],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
